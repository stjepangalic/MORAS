CHIP Eqto16 {
	IN a, b;
	OUT out[16];

	PARTS:
	Not(in = a, out = na);
	Not(in = b, out = nb);

	And(a = na, b = nb, out = oban);
	And(a = a, b = b, out = oba);

	Inc16(in = false, out = jedinica);
	Or(a = oban, b = oba, out = sel);

	Mux16(a = jedinica, b = false, sel = sel, out = out);
	
}